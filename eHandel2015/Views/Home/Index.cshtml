@model IEnumerable<eHandel2015.Repository.Demo>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <table>
        <thead>
            <tr>
                <th>
                    id
                </th>
                <th>
                    name
                </th>
                <th>
                    desc
                </th>
            </tr>
        </thead>
        <tbody>
            
                @foreach (var item in Model)
                {
                    <tr>
                        <th>
                            @Html.DisplayFor(modelItem => item.Id)
                        </th>
                        <th>
                            @Html.DisplayFor(modelItem => item.DemoItem)
                        </th>
                        <th>
                            @Html.DisplayFor(modelItem => item.DemoDescription)
                        </th>
                    </tr>
                }   
        </tbody>
    </table>


</head>
<body>
    <div>
    </div>
</body>
</html>
